<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2008/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Livet ViewModelCommand(Non CanExecute)</Title>
      <Shortcut>lvcomn</Shortcut>
      <Author>Livet Project</Author>
      <Description>Create a ViewModelCommand(Non CanExecute)</Description>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>Livet.Commands</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>name</ID>
          <ToolTip>Command name</ToolTip>
          <Default>Do</Default>
        </Literal>
        <Literal Editable="false">
          <ID>ViewModelCommand</ID>
          <Function>SimpleTypeName(global::Livet.Commands.ViewModelCommand)</Function> 
        </Literal>
      </Declarations>
      <Code Language="csharp">
<![CDATA[
private $ViewModelCommand$ _$name$Command;

public $ViewModelCommand$ $name$Command
{
get
{
if (_$name$Command == null)
{
_$name$Command = new $ViewModelCommand$($name$);
}
return _$name$Command;
}
}

public void $name$()
{
$end$
}
]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>